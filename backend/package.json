{
  "name": "eventsync-backend",
  "version": "1.0.0",
  "description": "EventSync Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --env-file=.env src/app.ts",
    "dev:watch": "nodemon --watch src --ext ts,js,json --env-file=.env src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:migrate": "node dist/utils/migrate.js",
    "db:seed": "node dist/utils/seed.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "eventsync-shared": "file:../shared",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.2",
    "playwright": "^1.54.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/node-fetch": "^2.6.12",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
